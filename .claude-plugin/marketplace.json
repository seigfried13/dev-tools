{
    "name": "9icu-marketplace",
    "owner": {
        "name": "Spencer Blecha",
        "email": "sblecha@gmail.com"
    },
    "metadata": {
        "description": "Personal Curated Claude Code plugins: skills, workflows, and productivity tools",
        "version": "0.1.0"
    },
    "plugins": [
        {
            "name": "util-tools",
            "description": "Collection of utility skills including Excel, Word, PowerPoint, and PDF capabilities",
            "source": "./",
            "strict": false,
            "skills": [
                "./skills/document-skills/xlsx",
                "./skills/document-skills/docx",
                "./skills/document-skills/pptx",
                "./skills/document-skills/pdf"
            ]
        },
        {
            "name": "design-tools",
            "description": "Collection skills focusing on frontend design-oriented tasks and brand-guideline development and adherence",
            "source": "./",
            "strict": false,
            "skills": [
                "./skills/skill-creator",
                "./skills/canvas-design",
                "./skills/algorithmic-art",
                "./skills/internal-comms",
                "./skills/webapp-testing",
                "./slack-gif-creator",
                "./skills/theme-factory",
                "./skills/brand-guidelines"
            ],
            "mcpServers": "./.design-mcp.json"
        },
        {
            "name": "core-tools",
            "source": "./",
            "description": "Personal dev workflow core tools",
            "version": "0.1.0",
            "license": "MIT",
            "commands": [
                "./superpowers/commands/brainstorm.md",
                "./superpowers/commands/execute-plan.md",
                "./superpowers/commands/write-plan.md"
            ],
            "agents": [
                "./superpowers/agents/code-reviewer.md"
            ],
            "hooks": {
                "SessionStart": [
                    {
                        "matcher": "startup|resume|clear|compact",
                        "hooks": [
                            {
                                "type": "command",
                                "command": "${CLAUDE_PLUGIN_ROOT}/superpowers/hooks/session-start.sh"
                            }
                        ]
                    }
                ]
            },
            "skills": [
                "./superpowers/skills",
                "./anthropics/skills/mcp-builder"
            ],
            "mcpServers": "./.core-mcp.json"
        }
    ]
}